{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "firefox",
      "request": "launch",
      "name": "vuejs: chrome",
      "url": "http://localhost:3000",
      "webRoot": "${workspaceFolder}/src",
      "breakOnLoad": true,
      "sourceMapPathOverrides": {
        "webpack:///src/*": "${webRoot}/*"
      },
      "pathMappings": [
        {
          "url": "webpack://vue-black-dashboard/src/components",
          "path": "${workspaceFolder}/src/components"
        },
        {
          "url": "webpack://vue-black-dashboard/src/pages",
          "path": "${workspaceFolder}/src/pages"
        }
      ]
    }
  ]
}
